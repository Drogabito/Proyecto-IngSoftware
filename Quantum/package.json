{
  "name": "Quantum",
  "version": "0.0.1",
  "description": "Quantum",
  "main": "init.js",
  "scripts": {
    "app": "electron dist",
    "postinstall": "install-app-deps",
    "build": "npm run build:dev",
    "build:dev": "export NODE_ENV=development&&webpack --config config/webpack.dev.js --progress --profile",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.browser.js --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "start": "npm run build:dev && electron dist",
    "start:dev": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "watch:electron": "nodemon --exec \"npm run start\" --ext ts,scss,html --ignore dist/ --ignore publish/",
	"package-linux": "npm run build && ./node_modules/electron-packager/cli.js dist App --platform=linux --arch=all --out=dist-app --overwrite",
	"package-win": "npm run build && ./node_modules/electron-packager/cli.js dist App --platform=win32 --arch=all --out=dist-app --overwrite"
  },
